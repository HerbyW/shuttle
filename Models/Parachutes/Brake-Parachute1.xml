<PropertyList>

<!-- Brake Parachute -->

	<path>Main-Parachute.ac</path>

	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.0</y-m>
		<z-m>0.0</z-m>
		<pitch-deg>0</pitch-deg>
	</offsets>

	<animation>
		<type>scale</type>
		<x-offset>0.45</x-offset>
                <y-offset>0.45</y-offset>
                <z-offset>0.45</z-offset>                
	</animation>
	
	<animation>
		<type>select</type>
		<condition>            
	          <equals>
                   <property>sim/multiplay/generic/int[3]</property>
                   <value>1</value>
                  </equals>                    
                 </condition>
	</animation>

<animation>
  <type>translate</type>  
  <property>sim/multiplay/generic/float[0]</property>
  <interpolation>
    <entry>
      <ind>45</ind>
      <dep>-4</dep>
    </entry>
    <entry>
      <ind>43</ind>
      <dep>1</dep>
    </entry>
    <entry>
      <ind>42</ind>
      <dep>2</dep>
    </entry>
    <entry>
      <ind>41</ind>
      <dep>3</dep>
    </entry>
    <entry>
      <ind>40</ind>
      <dep>4</dep>
    </entry>
    <entry>
      <ind>39</ind>
      <dep>5</dep>
    </entry>
  </interpolation>
  <center>
   <x-m> 90.0</x-m>
   <y-m> 0.0</y-m>
   <z-m> 0.0</z-m>   
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
</animation>    

</PropertyList>
